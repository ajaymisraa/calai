{
  "version": 2,
  "buildCommand": "cd ../.. && npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "images": {
    "sizes": [16, 32, 64, 96, 128, 256, 384],
    "domains": ["*"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**"
      },
      {
        "protocol": "http",
        "hostname": "**"
      }
    ]
  },
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  }
}